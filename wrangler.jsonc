{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "house",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-09-30",
  "compatibility_flags": ["nodejs_compat"],
  "dev": {
    "port": 4321,
  },
  "env": {
    "main": {
      "assets": {
        "binding": "ASSETS",
        "directory": "./dist",
        "html_handling": "drop-trailing-slash",
      },
    },
    "ageru": {
      "name": "kiroku",
      "main": "./src/ageru/main.ts",
      "r2_buckets": [
        {
          "binding": "KIROKU",
          "bucket_name": "kiroku",
        },
      ],
    },
    "dasu": {
      "name": "dasu",
      "main": "./src/content/loader/dasu.ts",
    },
  },
}
